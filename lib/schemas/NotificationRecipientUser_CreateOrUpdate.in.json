{
    "type": "object",
    "properties": {
        "resourceGroupName": {
            "required": true,
            "type": "string"
        },
        "serviceName": {
            "required": true,
            "type": "string",
            "minLength": 1,
            "maxLength": 50,
            "pattern": "^[a-zA-Z](?:[a-zA-Z0-9-]*[a-zA-Z0-9])?$"
        },
        "notificationName": {
            "required": true,
            "type": "string",
            "enum": [
                "RequestPublisherNotificationMessage",
                "PurchasePublisherNotificationMessage",
                "NewApplicationNotificationMessage",
                "BCC",
                "NewIssuePublisherNotificationMessage",
                "AccountClosedPublisher",
                "QuotaLimitApproachingPublisherNotificationMessage"
            ]
        },
        "userId": {
            "required": true,
            "type": "string",
            "minLength": 1,
            "maxLength": 80,
            "pattern": "^[^*#&+:<>?]+$"
        },
        "subscriptionId": {
            "required": true,
            "type": "string"
        },
        "api_version": {
            "required": true,
            "type": "string"
        }
    }
}